{
  "ROSTemplateFormatVersion": "2015-09-01",
  "Description": "Test Arms AlertContact„ÄÅAlertContactGroup",
  "Parameters": {
    "ContactName": {
      "Type": "String",
      "Default": "mytest"
    },
    "Email": {
      "Type": "String"
    },
    "ContactGroupName": {
      "Type": "String",
      "Default": "mytest"
    }
  },
  "Resources": {
    "AlertContact": {
      "Type": "ALIYUN::ARMS::AlertContact",
      "Properties": {
        "ContactName": {
          "Ref": "ContactName"
        },
        "Email": {
          "Ref": "Email"
        }
      }
    },
    "AlertContactGroup": {
      "Type": "ALIYUN::ARMS::AlertContactGroup",
      "Properties": {
        "ContactIds": [
          {
            "Fn::GetAtt": [
              "AlertContact",
              "ContactId"
            ]
          }
        ],
        "ContactGroupName": {
          "Ref": "ContactGroupName"
        }
      }
    }
  },
  "Outputs": {
    "ContactGroupId": {
      "Value": {
        "Fn::GetAtt": [
          "AlertContactGroup",
          "ContactGroupId"
        ]
      }
    }
  }
}