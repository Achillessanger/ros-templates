{
  "ROSTemplateFormatVersion": "2015-09-01",
  "Description": "Test CR NameSpace",
  "Parameters": {
    "Namespace": {
      "Type": "String",
      "Default": "mytest"
    },
    "DefaultVisibility": {
      "Type": "String",
      "Default": "PRIVATE",
      "AllowedValues": [
        "PUBLIC",
        "PRIVATE"
      ]
    },
    "AutoCreate": {
      "Type": "Boolean",
      "Default": false,
      "AllowedValues": [
        true,
        false
      ]
    }
  },
  "Resources": {
    "NameSpace": {
      "Type": "ALIYUN::CR::Namespace",
      "Properties": {
        "Namespace": {
          "Ref": "Namespace"
        },
        "DefaultVisibility": {
          "Ref": "DefaultVisibility"
        },
        "AutoCreate": {
          "Ref": "AutoCreate"
        }
      }
    }
  },
  "Outputs": {
    "NamespaceId": {
      "Value": {
        "Fn::GetAtt": [
          "NameSpace",
          "NamespaceId"
        ]
      }
    }
  }
}