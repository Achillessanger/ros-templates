{
  "ROSTemplateFormatVersion": "2015-09-01",
  "Description": "Test ServerlessKubernetesCluster",
  "Parameters": {
    "Name": {
      "Type": "String",
      "Default": "mytest"
    }
  },
  "Resources": {
    "Cluster": {
      "Type": "ALIYUN::CS::ServerlessKubernetesCluster",
      "Properties": {
        "Name": {
          "Ref": "Name"
        },
        "Tags": [
          {
            "Key": "Serverless"
          }
        ]
      }
    }
  },
  "Outputs": {
    "ClusterId": {
      "Value": {
        "Fn::GetAtt": [
          "Cluster",
          "ClusterId"
        ]
      }
    },
    "TaskId": {
      "Value": {
        "Fn::GetAtt": [
          "Cluster",
          "TaskId"
        ]
      }
    }
  }
}