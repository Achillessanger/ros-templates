{
  "ROSTemplateFormatVersion": "2015-09-01",
  "Description": "Test SLB AccessControl",
  "Parameters": {
    "AclName": {
      "Type": "String",
      "Default": "mytest"
    },
    "AddressIPVersion": {
      "Type": "String",
      "Default": "ipv4"
    }
  },
  "Resources": {
    "AccessControl": {
      "Type": "ALIYUN::SLB::AccessControl",
      "Properties": {
        "AddressIPVersion": {
          "Ref": "AddressIPVersion"
        },
        "AclName": {
          "Ref": "AclName"
        },
        "AclEntrys": [
          {
            "entry": "0.0.0.0"
          }
        ]
      }
    }
  },
  "Outputs": {
    "AclId": {
      "Value": {
        "Fn::GetAtt": [
          "AccessControl",
          "AclId"
        ]
      }
    }
  }
}