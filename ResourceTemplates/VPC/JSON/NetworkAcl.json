{
  "ROSTemplateFormatVersion": "2015-09-01",
  "Parameters": {
    "VpcName": {
      "Type": "String",
      "Default": "mytest"
    },
    "VpcCidrBlock": {
      "Type": "String",
      "Default": "192.168.0.0/16"
    },
    "VSwitchName": {
      "Type": "String",
      "Default": "mytest"
    },
    "VswCidrBlock": {
      "Type": "String",
      "Default": "192.168.10.0/24"
    },
    "ZoneId": {
      "Type": "String",
      "AssociationProperty": "ALIYUN::ECS::Instance::ZoneId"
    },
    "NetworkAclName": {
      "Type": "String",
      "Description": "The name of the network ACL.\nThe name must be 2 to 128 characters in length and can contain letters, digits, periods\n(.), underscores (_), and hyphens (-). The name must start with a letter and cannot\nstart with http:// or https://.",
      "Default": null
    },
    "Description": {
      "Type": "String",
      "Description": "The description of the network ACL.\nThe description must be 2 to 256 characters in length. The description must start\nwith a letter but cannot start with http:// or https://.",
      "Default": null
    },
    "IngressAclEntries": {
      "Type": "Json",
      "Description": "The list of ingress network ACL entries.",
      "MaxLength": 20,
      "Default": null
    },
    "EgressAclEntries": {
      "Type": "Json",
      "Description": "The list of egress network ACL entries.",
      "MaxLength": 20,
      "Default": null
    }
  },
  "Resources": {
    "Vpc": {
      "Type": "ALIYUN::ECS::VPC",
      "Properties": {
        "VpcName": {
          "Ref": "VpcName"
        },
        "CidrBlock": {
          "Ref": "VpcCidrBlock"
        }
      }
    },
    "VSwitch": {
      "DependsOn": "Vpc",
      "Type": "ALIYUN::ECS::VSwitch",
      "Properties": {
        "VpcId": {
          "Ref": "Vpc"
        },
        "VSwitchName": {
          "Ref": "VSwitchName"
        },
        "ZoneId": {
          "Ref": "ZoneId"
        },
        "CidrBlock": {
          "Ref": "VswCidrBlock"
        }
      }
    },
    "NetworkAcl": {
      "Type": "ALIYUN::VPC::NetworkAcl",
      "Properties": {
        "IngressAclEntries": {
          "Ref": "IngressAclEntries"
        },
        "Description": {
          "Ref": "Description"
        },
        "VpcId": {
          "Ref": "Vpc"
        },
        "EgressAclEntries": {
          "Ref": "EgressAclEntries"
        },
        "NetworkAclName": {
          "Ref": "NetworkAclName"
        }
      }
    },
    "NetworkAclAssociation": {
      "DependsOn": "NetworkAcl",
      "Type": "ALIYUN::VPC::NetworkAclAssociation",
      "Properties": {
        "NetworkAclId": {
          "Ref": "NetworkAcl"
        },
        "Resources": [
          {
            "ResourceId": {
              "Ref": "VSwitch"
            },
            "ResourceType": "VSwitch"
          }
        ]
      }
    }
  },
  "Outputs": {
    "NetworkAclId": {
      "Description": "The ID of the network ACL.",
      "Value": {
        "Fn::GetAtt": [
          "NetworkAcl",
          "NetworkAclId"
        ]
      }
    },
    "NetworkAclEntryName": {
      "Description": "The name of the inbound rule.",
      "Value": {
        "Fn::GetAtt": [
          "NetworkAcl",
          "NetworkAclEntryName"
        ]
      }
    }
  }
}